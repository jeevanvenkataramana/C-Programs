#include<iostream>
using namespace std;
struct Node
  {
     int data;
     struct Node *next;
  }

int main()
{
	int n;
	cout<<endl<<"Enter no of elements:";
	cin>>n;
	Node* head = new Node;
	head = nullptr;

	cout<<endl<<"Enter elements:"<<endl;
	for(int i=0; i<n; i++)
	{
		cin>>a[i];
		head==Insert(head, a[i]);
	}

	Node *temp=new Node;
	temp=head;
	while(temp!=nullptr)
	{
		cout<<temp->data;
		temp=temp->next;
	}

	return 0;
}

Node* Insert(Node *head,int data)
{
  // Complete this method

    Node* new_node = new Node;
    new_node->data=data;
    new_node->next = nullptr;

    if(head == nullptr)
    {
        cout<<"First"<<endl;
        head = new_node;
        return new_node;
    }
    else
    {
           Node* pos = new Node;
            pos = head;
           while(pos->next!=nullptr)
           {
               pos=pos->next;
            }

            pos->next=new_node;
            return head;
    }
}
